@model IEnumerable<SurfsUpClassLibrary.Models.Renting>

@{
    ViewData["Title"] = "Index";
}

<h1>Renting page</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SurfsUpUser.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Board)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SurfsUpUser.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Board.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
        </tr>
}
    </tbody>
</table>
